{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		},
    {
      "name": "page-notitle",
      "title": "Page: No title",
      "postTypes": [
        "page"
      ]
    },
    {
      "name": "front-page",
      "title": "Home",
      "postTypes": [
        "page"
      ]
    }
	],
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "query",
			"title": "Query (The Loop)",
			"area": "uncategorized"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"gradients": [
				{
					"name": "Blue to Magenta",
					"slug": "blue-to-magenta",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--dark-blue), var(--wp--preset--color--magenta))"
				}
			],
			"link": true,
			"palette": [
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				},
				{
					"name": "Dark-Gray",
					"slug": "dark-gray",
					"color": "#212529"
				},
        {
          "name": "Gray",
          "slug": "gray",
          "color": "#ABB8C3"
        },
        {
          "name": "Light Gray",
          "slug": "light-gray",
          "color": "#848484"
        },
				{
					"name": "Charcoal",
					"slug": "charcoal",
					"color": "#264653"
				},
				{
					"name": "Green",
					"slug": "green",
					"color": "#2A9D8F"
				},
				{
					"name": "Yellow",
					"slug": "yellow",
					"color": "#E9C46A"
				},
				{
					"name": "Orange",
					"slug": "orange",
					"color": "#F4A261"
				},
				{
					"name": "Red",
					"slug": "red",
					"color": "#E76F51"
				},
        {
          "name": "Dark Blue",
          "slug": "dark-blue",
          "color": "#251F6E"
        },
				{
					"name": "White",
					"slug": "white",
					"color": "#FFFFFF"
				},
        {
          "name": "Magenta",
          "slug": "magenta",
          "color": "#DB23B3"
        },
				{
					"name": "Primary",
					"slug": "primary",
					"color": "var(--wp--preset--color--dark-blue)"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "var(--wp--preset--color--light-gray)"
				},
				{
					"name": "Foreground",
					"slug": "foreground",
					"color": "var(--wp--preset--color--black)"
				},
				{
					"name": "Background",
					"slug": "background",
					"color": "var(--wp--preset--color--white)"
				}
			]
		},
		"layout": {
			"contentSize": "650px",
			"wideSize": "1536px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "0.562rem",
					"slug": "9",
					"name": "1"
				},
				{
					"size": "1.125rem",
					"slug": "18",
					"name": "2"
				},
				{
					"size": "1.687rem",
					"slug": "27",
					"name": "3"
				},
				{
					"size": "2.25rem",
					"slug": "36",
					"name": "4"
				},
				{
					"size": "2.812rem",
					"slug": "45",
					"name": "5"
				},
				{
					"size": "3.375rem",
					"slug": "54",
					"name": "6"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"lineHeight": true,
			"fontFamilies": [
        {
          "fontFamily": "\"Dongle\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", \"Liberation Sans\", sans-serif",
          "name": "Dongle",
          "slug": "dongle"
        },
        {
          "fontFamily": "\"Inter\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", \"Liberation Sans\", sans-serif",
          "name": "Inter",
          "slug": "inter"
        },
				{
					"fontFamily": "system-ui, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", \"Liberation Sans\", sans-serif",
					"name": "Sans Serif",
					"slug": "sans-serif"
				},
				{
					"fontFamily": "Cambria, Georgia, \"Times New Roman\", Times, serif",
					"name": "Serif",
					"slug": "serif"
				},
				{
					"fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\", \"Courier New\", monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"size": "1rem",
					"slug": "small"
				},
				{
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"size": "1.75rem",
					"slug": "large"
				},
        {
          "size": "2rem",
          "slug": "x-large"
        },
        {
          "size": "clamp(2.5rem, 3.5vw, 3.5rem)",
          "slug": "xx-large"
        }
			]
		},
		"useRootPaddingAwareAlignments": true,
		"custom": {
			"typography": {
				"line-height": {
					"body": 1.7,
					"heading": 1.3,
					"page-title": 1.1
				},
				"font-weight":{
					"light": "300",
					"normal": "400"
				},
				"font-size": {
					"huge": "clamp(1.6rem, 4vw, 4rem)",
					"gigantic": "clamp(2.625rem, 6vw, 6rem)",
					"colossal": "clamp(3.25rem, 8vw, 9rem)"
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"width": "1px",
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--blue)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				}
			},
			"core/search": {
				"border": {
					"width": "1px",
					"radius": "0px"
				},
				"color": {
					"text": "var(--wp--preset--color--blue)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--9)",
						"bottom": "var(--wp--preset--spacing--9)",
						"left": "var(--wp--preset--spacing--9)",
						"right": "var(--wp--preset--spacing--9)"
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)",
					"textTransform": "uppercase"
				}
			},
      "core/paragraph": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--inter)"
        }
      },
      "core/navigation": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--dongle)",
          "fontSize": "var(--wp--preset--font-size--x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--dark-blue)"
        }
      }
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--dark-gray)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--dark-blue)"
				}
			},
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--dark-blue)"
        }
      },
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dongle)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"lineHeight": "var(--wp--custom--typography--line-height--page-title)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dongle)",
					"fontSize": "var(--wp--custom--typography--font-size--huge)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dongle)",
					"fontSize": "var(--wp--custom--typography--font-size--xx-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.312rem",
			"padding": {
				"top": "var(--wp--preset--spacing--9)",
				"right": "var(--wp--preset--spacing--18)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--18)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--dongle)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		}
	}
}
